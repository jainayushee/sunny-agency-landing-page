{
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "react"],
    "extends": ["plugin:@typescript-eslint/recommended", "standard", "standard-react"],
    "rules": {
      "no-console": "error",
      "semi": "off",
      "react/jsx-no-bind": "off"
    }
  }
  